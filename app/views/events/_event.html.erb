<div id="<%= dom_id event %>">
  <p>
    <strong>Event name:</strong>
    <%= event.event_name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= event.description %>
  </p>

  <p>
    <strong>Event date:</strong>
    <%= event.event_date %>
  </p>

  <p>
    <strong>Event time:</strong>
    <%= event.event_time %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= event.price %>
  </p>

  <p>
    <strong>Event User Id:</strong>
    <%= event.user_id %>
  </p>

  <p>
    <strong>current User Id:</strong>
    <%= current_user.id %>
  </p>

</div>
